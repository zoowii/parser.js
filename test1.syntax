COMMENT := #"--.*(\\r\\n|\\n)"
STRING := #"(?:\"((?:\\.|[^\"])*)\"|'((?:\\.|[^'])*)')"
BOOLEAN := #"true|false"
IDENTIFIER := #"[a-zA-Z_\\$][a-zA-Z0-9_]*"
+ := #"\\+"
- := #"-"
* := #"\\*"
/ := #"/"
NUMBER := #"[+-]?(?:(0x[0-9a-fA-F]+|0[0-7]+)|((?:[0-9]+(?:\\.[0-9]*)?|\\.[0-9]+)(?:[eE][+-]?[0-9]+)?|NaN|Infinity))"
( := #"\\("
) := #"\\)"
[ := #"\\["
] := #"]"
. := #"."
, := #","
NEWLINE := #"(\\r\\n|\\n)+"
WHITESPACE := #"\\s+"
ILLEGAL := #".+"

